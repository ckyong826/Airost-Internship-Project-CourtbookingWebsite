<body>
  <form method="post" action="/createGame/:id" >
    <label for="fname">Event Name</label><br>
        <input type="text" id="fname" name="eventName"><br>
        <label for="lname">Time Start</label><br>
        <input type="text" id="lname" name="timeStart">
        <label for="lname">Time End</label><br>
        <input type="text" id="lname" name="timeEnd">
        <label for="lname">Maximum numbers of player to join</label><br>
        <input type="text" id="lname" name="playerMax">
        <label for="lname">Number of current players</label><br>
        <input type="text" id="lname" name="currentPlayer">
    <input type="hidden" name="id" value=<%= courtId %> >
    <input type="submit" value="Join" id="btn">
</form> 
